{
  "compilerOptions": {
    "moduleResolution": "bundler", // <- Fixes rollup/parseAst error
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react-jsx",
    "lib": ["DOM", "ESNext"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "strict": false,
    "noImplicitAny": false
  }
}
